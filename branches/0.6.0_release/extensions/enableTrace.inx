<inkscape-extension>
    <name>Begin recording a trace session</name>
    <id>org.ekips.filter.enableTrace</id>
	<dependency type="executable" location="extensions">enableTrace.py</dependency>
    <effect>
	<object-type>path</object-type>
	<effects-menu>
		<submenu _name="X-moto">
			<submenu _name="Other"/>
		</submenu>
	</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">enableTrace.py</command>
    </script>
</inkscape-extension>
