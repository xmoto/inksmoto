<inkscape-extension>
    <name>Convert into Background Block</name>
    <id>org.ekips.filter.addbackgroundblock</id>
	<dependency type="executable" location="extensions">addBackgroundBlock.py</dependency>
	<dependency type="executable" location="extensions">inkex.py</dependency>
    <param name="texture" type="string" _gui-text="Block texture">default</param>
    <effect>
		<object-type>path</object-type>
		<effects-menu>
			<submenu _name="X-moto">
				<submenu _name="Blocks"/>
			</submenu>
		</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">addBackgroundBlock.py</command>
    </script>
</inkscape-extension>
