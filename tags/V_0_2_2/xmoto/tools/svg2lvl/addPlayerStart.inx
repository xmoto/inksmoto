<inkscape-extension>
    <name>Convert into Player Start</name>
    <id>org.ekips.filter.addplayerstart</id>
	<dependency type="executable" location="extensions">addPlayerStart.py</dependency>
	<dependency type="executable" location="extensions">inkex.py</dependency>
    <effect>
		<object-type>path</object-type>
		<effects-menu>
			<submenu _name="X-moto">
				<submenu _name="Entities"/>
			</submenu>
		</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">addPlayerStart.py</command>
    </script>
</inkscape-extension>
