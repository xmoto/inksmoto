<inkscape-extension>
    <name>Set level properties</name>
    <id>org.ekips.filter.levelproperties</id>
	<dependency type="executable" location="extensions">addLevelInfosGtk.py</dependency>
    <effect>
		<object-type>path</object-type>
		<effects-menu>
			<submenu _name="X-moto">
				<submenu _name="Level"/>
			</submenu>
		</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">addLevelInfosGtk.py</command>
    </script>
</inkscape-extension>
