<inkscape-extension>
    <name>Sprite</name>
    <id>org.ekips.filter.changesprite</id>
	<dependency type="executable" location="extensions">changeSpriteGtk.py</dependency>
    <effect>
		<object-type>path</object-type>
		<effects-menu>
			<submenu _name="X-moto">
				<submenu _name="Entities"/>
			</submenu>
		</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">changeSpriteGtk.py</command>
    </script>
</inkscape-extension>
