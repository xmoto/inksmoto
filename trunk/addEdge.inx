<?xml version="1.0" ?>
<inkscape-extension><name>Add an edge around the block</name><id>org.ekips.filter.addedge</id><dependency location="extensions" type="executable">addEdge.py</dependency><dependency location="extensions" type="executable">inkex.py</dependency><param name="edge_description" type="description">There can be up to two different edge textures for a block. One for the upper side of the block, and another for the down side of the block.</param><param _gui-text="Update the upper edge texture" name="update_upper" type="boolean">true</param><param _gui-text="Upper edge texture" name="texture" type="enum"><item>GrassAlt</item><item>Grass</item><item>GrayBricks</item><item>Ice1</item><item>BlueBricks</item><item>RedBricks</item></param><param _gui-text="Update the down edge texture" name="update_down" type="boolean">true</param><param _gui-text="Down edge texture" name="downtexture" type="enum"><item>GrassAlt</item><item>Grass</item><item>GrayBricks</item><item>Ice1</item><item>BlueBricks</item><item>RedBricks</item></param><effect><object-type>path</object-type><effects-menu><submenu _name="X-moto"><submenu _name="Blocks"/></submenu></effects-menu></effect><script><command interpreter="python" reldir="extensions">addEdge.py</command></script></inkscape-extension>