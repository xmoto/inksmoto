<inkscape-extension>
    <name>Paste</name>
    <id>org.ekips.filter.xmoto_paste</id>
	<dependency type="executable" location="extensions">xmoto_paste.py</dependency>
    <effect>
	<object-type>path</object-type>
	<effects-menu>
		<submenu _name="X-moto">
			<submenu _name="Other"/>
		</submenu>
	</effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">xmoto_paste.py</command>
    </script>
</inkscape-extension>
